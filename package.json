{
	"name": "stylelint-config-firefoxic",
	"description": "Stylelint shareable config for the firefoxic code guide",
	"version": "2.0.3",
	"type": "module",
	"exports": "./.stylelintrc",
	"dependencies": {
		"stylelint-codeguide": "^0.3.2"
	},
	"peerDependencies": {
		"stylelint": "^15.11.0"
	},
	"files": [
		"./.stylelintrc"
	],
	"engines": {
		"node": ">=18.16"
	},
	"license": "MIT",
	"author": {
		"name": "Sergey Artemov",
		"email": "firefoxic.dev@gmail.com"
	},
	"homepage": "https://github.com/firefoxic/stylelint-config-firefoxic#readme",
	"bugs": {
		"url": "https://github.com/firefoxic/stylelint-config-firefoxic/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/firefoxic/stylelint-config-firefoxic.git"
	},
	"keywords": [
		"style",
		"lint",
		"linter",
		"stylelint",
		"codeguide"
	],
	"scripts": {
		"prepare": "husky install",
		"lint:spaces": "ec",
		"lint:scripts": "eslint .",
		"lint": "pnpm /^lint:/",
		"pretest": "pnpm lint",
		"test": "node --test",
		"preversion": "pnpm test",
		"postversion": "pnpm publish",
		"postpublish": "git push origin --all && git push origin --tags"
	},
	"devDependencies": {
		"editorconfig-checker": "^5.1.2",
		"eslint": "^8.51.0",
		"husky": "^8.0.3",
		"stylelint": "^15.11.0"
	}
}
